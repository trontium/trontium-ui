{
  "name": "@trontium/ui",
  "version": "1.0.0",
  "description": "A React UI Library by zsq.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trontium/trontium-ui.git"
  },
  "sideEffects": [
    "dist/*",
    "esm/**/style/*",
    "lib/**/style/*",
    "src/**/style/*",
    "*.less"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && gulp",
    "build:types": "tsc -p tsconfig.build.json && cpr lib esm",
    "clean": "rimraf lib esm dist",
    "lint": "eslint --ext .ts,.tsx src",
    "size": "size-limit",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0",
    "async-validator": "4.2.5",
    "classnames": "2.5.1"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@babel/preset-typescript": "7.7.7",
    "@size-limit/file": "^12.0.0",
    "@size-limit/webpack": "^12.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__jest-dom": "5.14.5",
    "babel-jest": "29.7.0",
    "babel-plugin-import": "1.13.3",
    "cpr": "3.0.1",
    "cross-env": "7.0.3",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-babel": "8.0.0",
    "gulp-cssnano": "2.1.3",
    "gulp-less": "^5.0.0",
    "identity-obj-proxy": "3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "24.9.0",
    "less": "^4.1.3",
    "plop": "2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "3.0.0",
    "size-limit": "^12.0.0",
    "snapshot-diff": "0.6.1",
    "through2": "3.0.1",
    "ts-jest": "24.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
